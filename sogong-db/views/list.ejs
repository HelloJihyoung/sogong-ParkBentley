<html>
    <head>
        <meta charset="utf-8">
        <title>로그인</title>
    </head>
    <body>
        <h2>로그인하기</h2>
        <table border='1'>
            <colgroup>
                <col width='60'><col width='200'><col width='100'>
            </colgroup>
            <thead>
                <tr>
                    <th>NUM</th>
                    <th>PW</th>
                    <th>ID</th>
                </tr>
            </thead>
            <tbody>
                <% for(i = 0; i < list.length; i++) { %>
                <tr>
                    <td><%=i+1 %></td>
                    <td><%=list[i].TITLE %></td>
                    <td><%=list[i].ID %></td>
                </tr>
                <% } %>
            </tbody>
        </table boader='1'>
        
        <button onclick="location.href='../login'">확인</button>
    </body>
</html>
